{
  "name": "translator",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "cd client && npm install",
    "client": "npm start --prefix client",
    "start": "NODE_OPTIONS='--loader ts-node/esm' node index.ts",
    "tsc": "tsc",
    "ts-node": "ts-node",
    "server": "nodenv index.ts --exec nodemon ts-node",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.0",
    "concurrently": "^7.4.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "deepl": "^1.0.13",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "~4.16.1",
    "express-pino-logger": "^7.0.0",
    "http-errors": "~1.6.3",
    "mariadb": "^3.0.1",
    "node-env-run": "^4.0.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^2.2.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "type": "module"
}
